{
    "browser_action": {
        "default_icon": "icon_32.png",
        "default_title": "SNKRS Explorer"
    },
    "description": "This extension will monitor the SNKRS website.",
    "icons": {
        "128": "icon_128.png",
        "16": "icon_16.png",
        "24": "icon_24.png",
        "32": "icon_32.png",
        "48": "icon_48.png"
    },
    "manifest_version": 2,
    "name": "SNKRS Explorer",
    "permissions": ["bookmarks", "notifications", "webRequest", "webRequestBlocking", "\u003Call_urls>"],
    "version": "1.0",
    "content_scripts": [{
        "matches": ["*://*.nike.com/*"],
        "js": ["main.js"]
    }]
}